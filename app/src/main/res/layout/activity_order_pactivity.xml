<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".orderP_activity">

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="408dp"
        android:layout_height="396dp"
        android:background="#B47D55"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/textView34"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="PESANAN"
            android:textAlignment="center"
            android:textColor="#542D10"
            android:textSize="29sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/meja_detail"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:text="textView"
            android:textAlignment="center"
            android:textColor="#542D10"
            android:textSize="20sp"
            android:textStyle="bold" />

        <View
            android:id="@+id/divider"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#3E230F" />

        <TextView
            android:id="@+id/pesanan1"
            android:layout_width="match_parent"
            android:layout_height="26dp"
            android:text="textView"
            android:textColor="#542D10"
            android:textSize="14sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/jml_pesanan1"
            android:layout_width="match_parent"
            android:layout_height="24dp"
            android:text="0"
            android:textColor="#542D10"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/price_pesanan1"
            android:layout_width="match_parent"
            android:layout_height="73dp"
            android:text="textView"
            android:textColor="#542D10"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/total_price1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="0"
            android:textAlignment="viewEnd"
            android:textColor="#542D10"
            android:textStyle="bold" />

        <View
            android:id="@+id/divider3"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#3E230F" />

        <TextView
            android:id="@+id/pesanan2"
            android:layout_width="match_parent"
            android:layout_height="26dp"
            android:text="textView"
            android:textColor="#542D10"
            android:textSize="14sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/jml_pesanan2"
            android:layout_width="match_parent"
            android:layout_height="24dp"
            android:text="0"
            android:textColor="#542D10"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/price_pesanan2"
            android:layout_width="match_parent"
            android:layout_height="73dp"
            android:text="textView"
            android:textColor="#542D10"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/total_price2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="0"
            android:textAlignment="viewEnd"
            android:textColor="#542D10"
            android:textStyle="bold" />

        <View
            android:id="@+id/divider4"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#3E230F" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nama Pemesan:"
        android:textColor="#542D10"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="397dp"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/nama_pelanggan"
        android:layout_width="362dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:hint="Ketik nama pemesan"
        android:singleLine="false"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <ImageButton
        android:id="@+id/save_nama_pelanggan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="72dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toTopOf="@+id/bayar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nama_pelanggan"
        app:layout_constraintVertical_bias="0.203"
        app:srcCompat="@drawable/simpan"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ImageButton
        android:id="@+id/back_menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="88dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/bayar"
        app:layout_constraintHorizontal_bias="0.301"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/save_nama_pelanggan"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/kmbli"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ImageButton
        android:id="@+id/bayar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="80dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/bayar"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>